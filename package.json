{
  "name": "testapp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "./node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "./node_modules/.bin/nest start --watch",
    "start:dev": "./node_modules/.bin/nest start --watch",
    "start:debug": "./node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:prod": "serverless deploy --env=production"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.7",
    "@nestjs/core": "^6.10.7",
    "@nestjs/platform-express": "^6.10.7",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.585.0",
    "aws-serverless-express": "^3.3.6",
    "dotenv": "^8.2.0",
    "google-auth-library": "^5.6.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "smtp-client": "^0.3.1",
    "uuidv4": "^6.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.12.9",
    "@nestjs/schematics": "^6.7.5",
    "@nestjs/testing": "^6.10.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "serverless": "^1.59.2",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-optimize": "^4.1.1-rc.1",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-plugin-warmup": "^4.7.1-rc.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
